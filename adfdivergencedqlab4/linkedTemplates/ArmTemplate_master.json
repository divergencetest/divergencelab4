{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adfdivergencedqlab4"
		},
		"InvoicesBlob_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'InvoicesBlob'"
		},
		"InvoicesDB_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'InvoicesDB'"
		},
		"WhenInputFileArrives_properties_LoopFiles_parameters_RecipientList": {
			"type": "array",
			"defaultValue": [
				"divergencetest@outlook.com"
			]
		},
		"WhenInputFileArrives_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/3a6713ad-fb61-40a1-8a95-cc80075db0ac/resourceGroups/rgdivergencedqlab4/providers/Microsoft.Storage/storageAccounts/sadivergencedqlab4"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adfdivergencedqlab4_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"InvoicesBlob_connectionString": {
						"value": "[parameters('InvoicesBlob_connectionString')]"
					},
					"InvoicesDB_connectionString": {
						"value": "[parameters('InvoicesDB_connectionString')]"
					},
					"WhenInputFileArrives_properties_LoopFiles_parameters_RecipientList": {
						"value": "[parameters('WhenInputFileArrives_properties_LoopFiles_parameters_RecipientList')]"
					},
					"WhenInputFileArrives_properties_typeProperties_scope": {
						"value": "[parameters('WhenInputFileArrives_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}